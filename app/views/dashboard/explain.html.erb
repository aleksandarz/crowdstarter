<div class="explain">

  <div class="startnow">
    <a href="<%=new_project_path%>">
      <button>Fund a project</button>
    </a>
  </div>

  <h1>Crowd-Sourced Funding</h1>

  <p class="tagline">
  Get funding for a group project or activity!
  </p>

  <% @example_projects.each do |project| %>
    <div class="project-box-container-left">
    <%= render :partial => "projects/item", :locals => {:project => project,
                                                        :inline_support => true}%>
    </div>
  <% end %>
</div>